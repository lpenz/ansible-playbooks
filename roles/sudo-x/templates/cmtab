# /etc/cryptmount/cmtab - encrypted filesystem information for cryptmount
# try 'man 8 cryptmount' or 'man 5 cmtab' for more details
dropboxcrypted {
    dev=/home/{{ ansible_ssh_user }}/Dropbox/private/crypted.vol
    dir=/home/{{ ansible_ssh_user }}/dropboxcrypted
    fstype=auto
    keyformat=luks
}
